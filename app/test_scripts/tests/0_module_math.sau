[use "math"]

[assert "module_math.sau > pi" [== math::pi 3.14159265359]]
[assert "module_math.sau > phi" [== math::phi 1.61803398875]]
[assert "module_math.sau > e" [== math::e 2.71828182845]]

[assert "module_math.sau > log" [== [math::log 4] 1.386294]]
[assert "module_math.sau > log2" [== [math::log2 10] 3.321928]]
[assert "module_math.sau > log10" [== [math::log10 8] 0.903090]]

[assert "module_math.sau > sin" [== [math::sin 1] 0.841471]]
[assert "module_math.sau > cos" [== [math::cos 1] 0.540302]]
[assert "module_math.sau > tan" [== [math::tan 1] 1.557408]]

[assert "module_math.sau > asin" [== [math::asin 1] 1.570796]]
[assert "module_math.sau > acos" [== [math::acos 1] 0.000000]]
[assert "module_math.sau > atan" [== [math::atan 1] 0.785398]]

[assert "module_math.sau > asinh" [== [math::asinh 1] 0.881374]]
[assert "module_math.sau > acosh" [== [math::acosh 1] 0.000000]]
[assert "module_math.sau > atanh" [== [math::atanh 0.5] 0.549306]]

[assert "module_math.sau > exp" [== [math::exp 8] 2980.957987]]
[assert "module_math.sau > sqrt" [== [math::sqrt 18] 4.242641]]

[assert "module_math.sau > abs" [== [math::abs -2.5] 2.5]]
[assert "module_math.sau > ceil" [== [math::ceil 1.3] 2.0]]
[assert "module_math.sau > floor" [== [math::floor 1.9] 1.0]]

[assert "module_math.sau > pow" [== [math::pow 2 8] 256.000000]]